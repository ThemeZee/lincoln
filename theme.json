{
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "page-fullwidth",
			"title": "Page (Fullwidth)",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "page-no-title",
			"title": "No Title",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "page-no-title-fullwidth",
			"title": "No Title (Fullwidth)",
			"postTypes": [
				"page"
			]
		},
		{
			"name": "page-sidebar-right",
			"title": "Sidebar Right",
			"postTypes": [
				"page"
			]
		}
	],
	"settings": {
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"background": true,
			"text": true,
			"link": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": true,
			"defaultPalette": true,
			"palette": [
				{
					"slug": "primary",
					"color": "#1f85eb",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#0b71d7",
					"name": "Secondary"
				},
				{
					"slug": "light-gray",
					"color": "#f0f0f0",
					"name": "Light Gray"
				},
				{
					"slug": "medium-gray",
					"color": "#e0e0e0",
					"name": "Medium Gray"
				},
				{
					"slug": "gray",
					"color": "#808080",
					"name": "Gray"
				},
				{
					"slug": "dark-gray",
					"color": "#303030",
					"name": "Dark Gray"
				}
			]
		},
		"layout": {
			"contentSize": "800px",
			"wideSize": "1080px"
		},
		"typography": {
			"dropCap": true,
			"customFontSize": true,
			"letterSpacing": true,
			"lineHeight": true,
			"fontStyle": true,
			"fontWeight": true,
			"textTransform": true,
			"fontFamilies": [
				{
					"fontFamily": "Roboto, Arial, Verdana",
					"name": "Roboto",
					"slug": "roboto"
				}
			],
			"fontSizes": [
				{
					"size": "14px",
					"slug": "tiny",
					"name": "Tiny"
				},
				{
					"size": "16px",
					"slug": "small",
					"name": "Small"
				},
				{
					"size": "18px",
					"slug": "base",
					"name": "Base"
				},
				{
					"size": "20px",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"size": "clamp(20px, calc(1rem + 0.83vw), 24px)",
					"slug": "large",
					"name": "Large"
				},
				{
					"size": "clamp(22px, calc(1rem + 1.25vw), 28px)",
					"slug": "x-large",
					"name": "Extra Large"
				},
				{
					"size": "clamp(24px, calc(1rem + 1.67vw), 36px)",
					"slug": "xx-large",
					"name": "Extra Extra Large"
				},
				{
					"size": "clamp(28px, calc(1rem + 2.5vw), 48px)",
					"slug": "huge",
					"name": "Huge"
				}
			]
		},
		"spacing": {
			"blockGap": "1.5rem",
			"padding": true,
			"margin": true,
			"units": [ "px", "em", "rem", "vh", "vw", "%" ],
			"spacingSizes": [
				{
				  "name": "X-Small",
				  "slug": 30,
				  "size": "clamp(0.25rem, 4vw, 0.75rem)"
				},
				{
				  "name": "Small",
				  "slug": 40,
				  "size": "clamp(0.5rem, 4vw, 1rem)"
				},
				{
				  "name": "Medium",
				  "slug": 50,
				  "size": "clamp(0.75rem, 4vw, 1.5rem)"
				},
				{
				  "name": "Large",
				  "slug": 60,
				  "size": "clamp(0.75rem, 4vw, 2rem)"
				},
				{
				  "name": "X-Large",
				  "slug": 70,
				  "size": "clamp(1rem, 4vw, 3rem)"
				},
				{
				  "name": "XX-Large",
				  "slug": 80,
				  "size": "clamp(1.25rem, 4vw, 5rem)"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/archives": {
				"elements": {
                    "link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				":hover": {
					"color": {
						"background": "var(--wp--preset--color--secondary)"
					}
				}
			},
			"core/categories": {
				"elements": {
                    "link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			},
			"core/post-content": {
				"typography": {
					"fontSize": "clamp(16px, calc(1rem + 0.16vw), 19px)"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "clamp(16px, calc(1rem + 0.16vw), 19px)"
				}
			},
			"core/post-title": {
				"elements": {
                    "link": {
						"color": {
							"text": "#202020"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--primary)"
							}
						}
					}
				}
			}
		},
		"color": {
			"background": "#fafafa",
			"text": "#202020"
		},
		"elements": {
			"heading": {
				"spacing": {
					"margin": {
						"top": "1.5rem",
						"bottom": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--roboto)",
					"lineHeight": "1.25"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--huge)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xx-large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "22px"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "20px"
				}
			},
			"caption": {
				"spacing": {
					"margin": {
						"top": "0.5rem",
						"bottom": "1.5rem"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--tiny)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"typography": {
			"fontSize": "17px",
			"fontFamily": "var(--wp--preset--font-family--roboto)",
			"lineHeight": "1.75"
		},
		"spacing": {
			"blockGap": "1.5rem"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header Large",
			"area": "header"
		},
		{
			"name": "header-small",
			"title": "Header Small",
			"area": "header"
		},
		{
			"name": "header-social-icons",
			"title": "Header with Social Icons",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer Large",
			"area": "footer"
		},
		{
			"name": "footer-small",
			"title": "Footer Small",
			"area": "footer"
		}
	]
}
